{
	"name": "hsb-service-utils",
	"version": "0.0.0",
	"description": "home-sandbox utilities shared between services",
	"author": "Akos Kun",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": "12.14.1"
	},
	"main": "build/index.js",
	"scripts": {
		"dev": "parcel watch src/*.js --target node --out-dir build --cache-dir .parcel-cache --public-url ./",
